{
    "command": [
        "/bin/bash",
        "-c",
        "chown -R keystone:keystone /var/log/keystone"
    ],
    "image": "undercloud-0.ctlplane.redhat.local:8787/rh-osbs/rhosp16-openstack-keystone:20200226.1",
    "net": "none",
    "start_order": 1,
    "user": "root",
    "volumes": [
        "/var/log/containers/keystone:/var/log/keystone:z",
        "/var/log/containers/httpd/keystone:/var/log/httpd:z"
    ]
}